{
    "name": "jrdoc-highlighting",
    "displayName": "Jrdoc Highlighting",
    "description": "Syntax highlighting in jrdoc multiline strings",
    "version": "0.8.3",
    "publisher": "SinelServers",
    "icon": "docs/logo.png",
    "engines": {
        "vscode": "^1.19.0"
    },
    "galleryBanner": {
        "color": "#dedede",
        "theme": "light"
    },
    "repository": {
        "url": "https://github.com/Joyte/jrdoc-highlighting"
    },
    "bugs": {
        "url": "https://github.com/Joyte/jrdoc-highlighting/issues"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "jrdoc",
        "es6",
        "jrdoc-highlighting",
        "highlighting",
        "syntax",
        "jrdoc"
    ],
    "contributes": {
        "grammars": [
            {
                "injectTo": [
                    "source.js",
                    "source.js.jsx",
                    "source.jsx",
                    "source.ts",
                    "source.tsx",
                    "source.vue",
                    "source.svelte"
                ],
                "scopeName": "inline.es6-jrdoc",
                "path": "./syntaxes/es6-inline-jrdoc.json",
                "embeddedLanguages": {
                    "meta.embedded.jrdoc": "jrdoc"
                }
            }
        ]
    }
}
